<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="netsec.ethz.ch.verifier">

    <uses-permission android:name="android.permission.CAMERA" />

    <uses-feature android:name="android.hardware.camera2" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".Initialisation.InitMainActivity" />
        <activity android:name=".Initialisation.ScanTPMActivity" />
        <activity android:name=".Initialisation.SendConfigActivity" />
        <activity android:name=".Initialisation.SendNonceActivity" />
        <activity android:name=".Initialisation.ScanFinalActivity" />
        <activity android:name=".LogVerification.LogVerifMainActivity" />
        <activity android:name=".LogVerification.ScanLogsActivity" />
        <activity android:name=".LogVerification.LogResultActivity" />
        <activity android:name=".KeyReplication.KeyRepMainActivity" />
        <activity android:name=".Initialisation.InitChoiceActivity" />
        <activity android:name=".LogVerification.LogChoiceActivity" />
        <activity android:name=".KeyReplication.ScanEncKeyActivity"></activity>
    </application>

</manifest>